[package]
name = "{{{packageName}}}"
version = "{{#lambdaVersion}}{{{packageVersion}}}{{/lambdaVersion}}"
authors = ["Shuntaro Nishizawa <shun.shobon@gmail.com> (https://s2n.tech)"]
{{#appDescription}}
description = "{{{.}}}"
{{/appDescription}}
license = "MIT"
edition = "2021"
repository = "https://github.com/shun-shobon/openai-openapi-rs.git"
{{#documentationUrl}}
documentation = "{{.}}"
{{/documentationUrl}}
homepage = "https://github.com/shun-shobon/openai-openapi-rs"

[features]
default = ["rustls-tls"]
native-tls = ["reqwest/native-tls"]
rustls-tls = ["reqwest/rustls-tls"]

[dependencies]
serde = "^1.0"
serde_derive = "^1.0"
{{#serdeWith}}
serde_with = "^2.0"
{{/serdeWith}}
serde_json = "^1.0"
url = "^2.2"
uuid = { version = "^1.0", features = ["serde"] }
reqwest = { version = "^0.11", features = ["json", "multipart"] }

[dev-dependencies]
tokio = { version = "1.24.1", features = ["macros", "rt-multi-thread"] }
